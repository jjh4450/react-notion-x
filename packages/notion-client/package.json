{
  "name": "custom-notlon-renderer-client",
  "description": "Robust TypeScript client for the unofficial Notion API.",
  "repository": "jjh4450/custom-notlon-renderer",
  "type": "module",
  "author": "JJH4450 <jjh4450git@gmail.com>, based on work by Travis Fischer <travis@transitivebullsh.it>",
  "version": "0.1.0",
  "license": "MIT",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "sideEffects": false,
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch --silent --onSuccess 'echo build successful'",
    "test": "ava"
  },
  "dependencies": {
    "got": "^11.8.1",
    "custom-notlon-renderer-types": "^0.1.0",
    "custom-notlon-renderer-utils": "^0.1.0",
    "p-map": "^5.3.0"
  },
  "ava": {
    "snapshotDir": ".snapshots",
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--no-warnings",
      "--experimental-specifier-resolution=node"
    ]
  }
}
